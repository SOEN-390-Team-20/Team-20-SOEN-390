{
  "name": "jevaisbienaller-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "lint:all": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "start": "cross-env NODE_ENV=production node ./bin/www",
    "dev": "cross-env NODE_ENV=development nodemon -L ./bin/www --ignore client --exec \"npm run lint:fix && node\"",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand --watch",
    "test:only": "cross-env NODE_ENV=test jest --verbose --runInBand",
    "build": "cd ./client && npm install && npm run build",
    "build:all": "npm install && cd ./client && npm install && npm run build",
    "dev:build": "npm run build && npm run dev",
    "start:build": "npm run build && npm run start",
    "start:test": "cross-env NODE_ENV=test node ./bin/www",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "cy:run": "start-server-and-test start:test http://localhost:3001 cypress:run",
    "cy:start": "start-server-and-test start:test http://localhost:3001 cypress:open"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.8",
    "mongoose-unique-validator": "^3.0.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "cypress": "^9.5.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.5.0",
    "nodemon": "^2.0.15",
    "start-server-and-test": "^1.14.0",
    "supertest": "^6.2.2",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "client",
      "cypress"
    ]
  }
}
